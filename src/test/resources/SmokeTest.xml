<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="SmokeTest">
 <test name="SmokeTest">
 
 	<parameter name="DepartCity" value="МИНСК" />
	<parameter name="ArriveCity" value="РИГА" />
	<parameter name="Months" value="3" />

	<groups>
		<run>
			<include name="tickets to both sides" />
			<exclude name="one way ticket" />
		</run>
	</groups>

 	<classes>
		<class name="by.htp.kyzniatsova.test.*" />
	</classes>
 </test>
</suite>
